                            <TextBlock.Effect>
                                <DropShadowEffect
                                    BlurRadius="3"
                                    Opacity="0.8"
                                    ShadowDepth="4"
                                    Color="Black" />
                            </TextBlock.Effect>
                        </TextBlock>
